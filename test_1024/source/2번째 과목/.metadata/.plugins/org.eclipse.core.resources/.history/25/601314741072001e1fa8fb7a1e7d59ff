package test02;

public class Test02_1 {
	public static void main(String[] args) {
		int i = 0;
			int tr[] = new int[i];
			do {
				tr[i] = 3*(i+1);
				i++;
			} while(tr[i]>1000);
			
			for(int k=0;k<tr.length;k++) {
				System.out.println(tr[k]);
			}
			
